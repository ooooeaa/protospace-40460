<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
            <%= @user.name %>さんの情報
      </h2>
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= link_to @user.name, user_path %>
          </tr>
          <tr>
            <th class="table__col1">プロフィール</th>
            <td class="table__col2"><%= link_to @user.profile, user_path %>
          </tr>
          <tr>
            <th class="table__col1">所属</th>
            <td class="table__col2"><%= link_to @user.position, user_path %>
          </tr>
          <tr>
            <th class="table__col1">役職</th>
            <td class="table__col2"><%= link_to @user.occupation, user_path%>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        <%= link_to @user.name, user_path %>さんのプロトタイプ
      </h2>
      <div class="user__card">
        <%# 部分テンプレートでそのユーザーが投稿したプロトタイプ投稿一覧を表示する %>
         <% @user.prototypes.each do |prototype| %>
  <%= render 'prototypes/prototype', prototype: prototype %>
<% end %>
      </div>
    </div>
  </div>
</div>